<h3>Detected Systems:</h3>
<div class="item">
        <div class="itemContent">
                {{:~link(systemname1,{'setarget1': 1})}}
                <br>
                {{:~link(systemname2,{'setarget2': 1})}}
        </div>
</div>
<h3>Current System:</h3>
<div class="item">
  <div class="itemContent">
    {{:systemname}}<br>
    {{:systemtype}}<br>
    {{:systemlum}}<br>
  </div>

  {{if !systembinary == '-'}}
  <div class = "itemContent" align ="right">
   Binary System <br>
   {{:systembinary}}<br>
  </div>
  {{/if}}
<br>
  <div class=itemContent" align="center">
    {{:~link('Clear scans',{ 'clear': 1})}}
  </div>
</div>
